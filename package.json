    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-form-data": "^2.0.16",
    "express-http-context": "^1.2.4",
    "form-data": "^4.0.0",
    "jsonwebtoken": "^8.5.1",
    "lodash.pick": "^4.4.0",
    "lodash.pickby": "^4.6.0",
    "log4js": "^6.3.0",
    "mongoose": "^5.12.2",
    "multer": "^1.4.2",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "^0.9.0",
    "tsconfig-paths": "^3.9.0",
    "uuid-random": "^1.3.2"
