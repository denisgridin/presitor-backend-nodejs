{
  "compilerOptions": {
    "target": "ES2018",
    "esModuleInterop": true,
    "outDir": "dist",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "rootDir": "src",
    "moduleResolution": "node",
    "baseUrl": "./dist",
    "types": [
      "node"
    ],
    "paths": {
      "*": [
        "*"
      ],
      "~/*": [
        "./src/*"
      ],
      "@/*": [
        "./src/*"
      ],
      "utils/*": [ "./src/utils/*" ],
      "interface/*": [ "./src/interface/*" ],
      "model/*": [ "./src/model/*" ],
      "validators/*": [ "./src/validators/*" ]
    }
  },
  "sourceMap": true
}
