{
  "compilerOptions": {
    "esModuleInterop": true,
    "outDir": "dist",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "rootDir": "src",
    "moduleResolution": "node",
    "baseUrl": "./dist",
    "types": [
      "node"
    ],
    "paths": {
      "*": [
        "*"
      ],
      "utils/*": [ "./src/utils/*" ],
      "interface/*": [ "./src/interface/*" ],
      "model/*": [ "./src/model/*" ],
      "validators/*": [ "./src/validators/*" ]
    },
    "exclude": [
      "node_modules",
      "dist",
      "@types",
      "tests",
      "examples",
      "_src",
      "__tests__"
    ]
  },
  "sourceMap": true
}
